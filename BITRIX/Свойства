//состав
var arr = ["velvet", "costume_jewelry", "beads", "hematite", "tree", "pearl", "skin", "brass", "copper", "metal", "natural_stones", "polymer_clay", "silver", "spinel", "jewelry_steel"];
var n = 0;//если уже есть несколько свойств
var nn = 0;//если уже есть несколько свойств
var count = arr.length;//если пустое свойства
for (let index = 0; index < count; index++) {
    document.getElementById('propedit_add_btn').click();
}



arr.forEach(function(element){
    document.getElementById('PROPERTY_VALUES_XML_n'+n).value = element;
    n++;
});

