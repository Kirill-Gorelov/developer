arr.forEach(function(element){
    document.getElementById('PROPERTY_VALUES_VALUE_n'+nn).value = element;
    nn++;
});

//бренд
var arr = ["anat", "anna_slavutina", "bngl", "byckovski", "ceramicbyava", "dasha_nachinkina", "dore_art", "e_g_m", "ekaterina_tolstaya", "ezhen", "fjord", "hairytale_stories", "honey_hany", "irina_volchek", "kalenik", "karavai", "kv_mode", "label_d", "lisa_smith", "maevsky", "mdclutch", "mechta_babaeva", "ms_marble", "natasha_gelb", "nevermind_jewelry", "vedovka", "waves_gems", "yauheniya_belskaya"];
var n = 0;//если уже есть несколько свойств
var nn = 0;//если уже есть несколько свойств
var count = arr.length - 5 - n;//если пустое свойства
for (let index = 0; index < count; index++) {
    document.getElementById('propedit_add_btn').click();
}

//n = 0;
arr.forEach(function(element){
    document.getElementById('PROPERTY_VALUES_XML_n'+n).value = element;
    n++;
});

