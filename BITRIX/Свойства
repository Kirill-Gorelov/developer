var arr = [];
document.querySelectorAll('.bx-digit-cell').forEach(function(element){
    //console.log(element.innerHTML);
    if (document.getElementById('PROPERTY_VALUES_XML_'+element.innerHTML) != null) {
        let v = document.getElementById('PROPERTY_VALUES_XML_'+element.innerHTML).value;
        //console.log(v);
        arr.push(v);
    }
});

console.log(arr);

//
var arr = [];
document.querySelectorAll('.bx-digit-cell').forEach(function(element){
    //console.log(element.innerHTML);
    if (document.getElementById('PROPERTY_VALUES_XML_'+element.innerHTML) != null) {
        let v = document.getElementById('PROPERTY_VALUES_VALUE_'+element.innerHTML).value;
        //console.log(v);
        arr.push(v);
    }
});

console.log(arr);


